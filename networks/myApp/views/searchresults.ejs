        <button onclick="location.href = '/tennis';" id="tennis" class="btn btn-secondary ml-3"> View </button>
     <% } %>
     <br>
        
     <% } %>
     <% if( xy.length ==0  ){ %>
        <h1 style="color:white">Not found</h1>;
       <!-- <window class="print">Not Found</window> -->
   <% } %> 
</body>
</html>