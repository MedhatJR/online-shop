            <br>
            <label id ="label2" class="ml-4 my-2">The Sun and Her Flowers</label>
            <br>
            <button onclick="location.href = '/sun';" id="sun" class="btn btn-secondary ml-3"> View </button>
                                <% } %> 
                                <br>
                              
     <% if(xy[i] === "Boxing Bag"){ %>
        <img id="image" src="/boxing.jpg" width="193" height="300">
        <br>
        <label id="label1" class="ml-4 my-2">Boxing Bag</label>
        <br>
        <button onclick="location.href = '/boxing';" id="boxing" class="btn btn-secondary ml-3"> View </button>
     <% } %>  
     <br>
                            
     <% if(xy[i] === "Tennis Racket"){ %>
        <img id="image" src="/tennis.jpg" width="193" height="300">
